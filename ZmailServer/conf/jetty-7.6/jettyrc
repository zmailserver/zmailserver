JAVA_OPTIONS="-DSTART=${JETTY_HOME}/etc/start.config -DSTOP.PORT=7867 -DSTOP.KEY=stop -Dzmail.config=/opt/zmail/conf/localconfig.xml -Djava.library.path=/opt/zmail/lib -Djava.endorsed.dirs=${JETTY_HOME}/common/endorsed"
JETTY_CONSOLE=/opt/zmail/log/jetty.out
JETTY_RUN=/opt/zmail/log
JETTY_ARGS=" --ini OPTIONS=Server,jsp,jmx,resources,websocket,ext,jta,plus,rewrite,monitor"
CONFIGS="etc/jetty.xml"