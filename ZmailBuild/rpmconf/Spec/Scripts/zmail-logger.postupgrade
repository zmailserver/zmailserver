if [ -L /opt/zimbra/logger/mysql ]; then
  rm -f /opt/zimbra/logger/mysql
fi

PKG=`echo ${RECEIPT_PATH} | sed -e 's|.*/\(zimbra-logger.[^/]*pkg\)/.*|\1|' | sed -e 's/\./-/'`
D=`date +%s`
echo "${D}: UPGRADED ${PKG}" >> /opt/zimbra/.install_history

exit 0
