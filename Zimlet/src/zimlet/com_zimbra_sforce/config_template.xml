<zimletConfig name="com_zimbra_sforce" version="3.4">
    <global>
        <property name="allowedDomains">*.salesforce.com,*.sforce.com</property>
        <property name="preauthDomainKey">PRE_AUTH</property>
        <property name="redirectUser">USER</property>
        <property name="SF_API_URL">https://www.salesforce.com/services/Soap/c/17.0</property>
        <!--
        <property name="hasSensitiveData">true</property>
          Set hasSensitiveData to true to disable this Zimlet when Zimbra Web Client is used in http mode.
            PS:
            - This property is ignored in Zimbra Desktop and in localhost
            - Supported from Zimbra 7.1.3 onwards
        -->
        <property name="hasSensitiveData">true</property>
    </global>
</zimletConfig>