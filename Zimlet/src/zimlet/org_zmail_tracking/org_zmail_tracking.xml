<zimlet name="com_zimbra_tracking"
        version="1.6"
        label="${msg.label}"
        description="${msg.description}">
    <include>shipping.js</include>
    <handlerObject>com_zimbra_tracking_HandlerObject</handlerObject>
    <contentObject type="tracking">
        <matchOn>
            <regex>Overrode in shipping.js</regex>
        </matchOn>
    </contentObject>
</zimlet>
