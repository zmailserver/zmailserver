<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
        version="2.0">

    <tlib-version>1.0</tlib-version>
    <short-name>zdf</short-name>
    <uri>org.zmail.cs.offline.jsp</uri>
    <function>
        <name>isValid</name>
        <function-class>
            org.zmail.cs.offline.jsp.FormBean
        </function-class>
        <function-signature>
            boolean isValid(org.zmail.cs.offline.jsp.FormBean, java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>reload</name>
        <function-class>
            org.zmail.cs.offline.jsp.FormBean
        </function-class>
        <function-signature>
            void reload(org.zmail.cs.offline.jsp.FormBean)
        </function-signature>
    </function>
    <function>
        <name>doRequest</name>
        <function-class>
            org.zmail.cs.offline.jsp.FormBean
        </function-class>
        <function-signature>
            void doRequest(org.zmail.cs.offline.jsp.FormBean)
        </function-signature>
    </function>
    <function>
        <name>getLocalConfig</name>
        <function-class>
            org.zmail.cs.offline.jsp.PageBean
        </function-class>
        <function-signature>
            String getLocalConfig(java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>checkAuthToken</name>
        <function-class>
            org.zmail.cs.offline.jsp.PageBean
        </function-class>
        <function-signature>
            boolean checkAuthToken(javax.servlet.http.HttpServletRequest)
        </function-signature>
    </function>
    <function>
        <name>isPrism</name>
        <function-class>
            org.zmail.cs.offline.jsp.PageBean
        </function-class>
        <function-signature>
            boolean isPrism(java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>getBaseUri</name>
        <function-class>
            org.zmail.cs.offline.jsp.PageBean
        </function-class>
        <function-signature>
            boolean getBaseUri()
        </function-signature>
    </function>
    <function>
        <name>addAuthToken</name>
        <function-class>
            org.zmail.cs.offline.jsp.PageBean
        </function-class>
        <function-signature>
            String addAuthToken(java.lang.String, javax.servlet.http.HttpServletRequest)
        </function-signature>
    </function>
    <function>
        <name>onLogin</name>
        <function-class>
            org.zmail.cs.offline.jsp.ClientEventBean
        </function-class>
        <function-signature>
            void onLogin(org.zmail.cs.offline.jsp.ClientEventBean)
        </function-signature>
    </function>
</taglib>
